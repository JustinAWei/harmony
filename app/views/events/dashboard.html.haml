!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title Harmony
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
    %link{:href => "https://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.7/leaflet.css", :rel => "stylesheet"}/
    %script{:src => "https://cdnjs.cloudflare.com/ajax/libs/leaflet/0.7.7/leaflet.js"}
    %script{:src => "https://www.mapquestapi.com/sdk/leaflet/v2.2/mq-map.js?key=KEY"}
    %script{:src => "https://www.mapquestapi.com/sdk/leaflet/v2.2/mq-geocoding.js?key=KEY"}
  %body
    = yield
